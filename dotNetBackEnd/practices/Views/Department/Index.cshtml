@{
}
<table border="1" cellpadding="2">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Salary</th>
    </tr>
    @foreach(var Dep in ViewData["Department"] as IList<Department>)
            {
                <tr>
                    <td>@Dep.ID</td>
                    <td>@Dep.Name</td>
                    <td>@Dep.Salary</td>
                </tr>
            }
</table>